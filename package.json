{
  "name": "mrpdf-express",
  "version": "1.0.0",
  "description": "Generate pdf from document on the web",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "dev": "ts-node src/index.ts",
    "build": "npm run clean && tsc",
    "lint": "eslint \"*/**/*.{js,ts}\"",
    "lint:fix": "eslint \"*/**/*.{js,ts}\" --fix",
    "postinstall": "tsc",
    "start": "node lib/index.js"
  },
  "keywords": [
    "pdf",
    "docusaurus",
    "vuepress",
    "mkdocs",
    "gitbook"
  ],
  "homepage": "https://github.com/humzaa-omar/mrpdf-express",
  "author": "Humzaa Omar",
  "license": "MIT",
  "dependencies": {
    "boxen": "^5.0.1",
    "chalk": "^3.0.0",
    "express": "^4.17.1",
    "puppeteer": "^2.1.1"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/node": "^15.14.7",
    "@types/puppeteer": "^2.1.6",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.0",
    "typescript": "^3.9.10"
  },
  "files": [
    "lib/**/*",
    "bin/**/*"
  ]
}